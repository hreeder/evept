{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "auth0-js": "^9.11.2",
    "aws-amplify": "^1.1.7",
    "aws-amplify-react": "^2.1.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.1.3",
    "env-cmd": "^9.0.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest-enzyme": "^7.0.1",
    "pako": "^1.0.6",
    "query-string": "^6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.6.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "timeago-react": "^2.0.0"
  },
  "scripts": {
    "build": "yarn build-sde && react-scripts build",
    "build-dev": "env-cmd yarn build",
    "build-sde": "node scripts/build-sde.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "extract-zip": "^1.6.7",
    "js-yaml": "^3.12.0",
    "node-fetch": "^2.2.0",
    "redux-mock-store": "^1.5.3"
  }
}
